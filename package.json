{
  "name": "porto-app",
  "version": "2.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=22.19.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "prebuild": "node scripts/update-sw-version.js",
    "build": "prisma generate && node scripts/update-sw-version.js && next build",
    "start": "next start",
    "deploy": "vercel build && vercel deploy --prebuilt",
    "deploy:prod": "vercel build --prod && vercel deploy --prebuilt --prod",
    "lint": "next lint",
    "test:integration": "node --test tests/integration/otp.test.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:perf": "playwright test tests/performance",
    "test:perf:ui": "playwright test tests/performance --ui"
  },
  "dependencies": {
    "@mapbox/polyline": "^1.2.1",
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/adapter-neon": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "dotenv": "^17.3.1",
    "graphql": "^16.8.1",
    "leaflet": "^1.9.4",
    "next": "16.1.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-leaflet": "^4.2.1",
    "swr": "^2.2.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@ducanh2912/next-pwa": "^10.2.9",
    "@playwright/test": "^1.58.2",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20",
    "@types/react": "19.2.13",
    "@types/react-dom": "19.2.3",
    "autoprefixer": "^10.0.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "postcss": "^8",
    "prisma": "^7.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "@types/react": "19.2.13",
    "@types/react-dom": "19.2.3"
  }
}
